() send_transfer(slice dest, int grams) impure inline {
    ~strdump("=== SEND_TRANSFER ===");
    ~dump(dest); ;; ðŸŸ¡ Ð»Ð¾Ð³Ð¸Ñ€ÑƒÐµÐ¼ slice

    var msg = begin_cell()
        .store_uint(0x10, 6)
