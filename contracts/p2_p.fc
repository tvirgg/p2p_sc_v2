        .store_uint(0, 1 + 4 + 4 + 64 + 32 + 1 + 1)
        .end_cell();

    ~strdump("=== RAW MSG TO SEND ===");
    ~dump(msg); ;; ðŸŸ¡ Ð»Ð¾Ð³Ð¸Ñ€ÑƒÐµÐ¼ ÑÐ¾Ð¾Ð±Ñ‰ÐµÐ½Ð¸Ðµ

    send_raw_message(msg, 1);
}

